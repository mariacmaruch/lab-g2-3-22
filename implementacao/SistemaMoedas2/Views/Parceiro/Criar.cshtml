@model Parceiro
@{
    ViewData["Title"] = "Cadastrar parceiro";
}

<div class="text-justify">
    <h3 class="display-4">Cadastrar parceiro</h3>
    <form asp-controller="Parceiro" asp-action="Criar" method="post">
        <div class="form-group mb-3">
            <label for="exampleInputNome">Nome</label>
            <input type="text" asp-for="Nome" class="form-control" id="exampleInputNome">
        </div>

        <div class="form-group mb-3">
            <label for="exampleInputEmail1">Email</label>
            <input type="email" asp-for="Email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
        </div>

        <div class="form-group mb-3">
            <label for="exampleInputPassword1">Senha</label>
            <input type="password" asp-for="Senha" class="form-control" id="exampleInputPassword1">
        </div>

        <div class="form-group mb-3">
            <label for="exampleInputCpf">CNPJ</label>
            <input type="text" asp-for="Cnpj" class="form-control" id="exampleInputCnpj">
        </div>

        <br />

        <div>
            <button type="submit" class="btn btn-primary">Cadastrar</button>
            <a role="button" class="btn btn-secondary" asp-controller="Parceiro" asp-action="Index">Voltar</a>
        </div>
    </form>

</div>